func GlobalSingleParameterIdentity(name string) Identity {
	return Identity{
		IsGlobalResource:  true,
		IdentityAttribute: name,
		Attributes: []IdentityAttribute{
			{
				Name:     "account_id",
				Required: false,
			},
			{
				Name:     name,
				Required: true,
			},
		},
		IsSingleParameter: true,
	}
}
