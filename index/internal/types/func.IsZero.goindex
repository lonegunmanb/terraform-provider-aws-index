package github.com/hashicorp/terraform-provider-aws/internal/types
import (
	"reflect"
)
func IsZero[T any](v T) bool {
	val := reflect.ValueOf(v)
	val = reflect.Indirect(val)

	if val.Kind() == reflect.Interface {
		val = val.Elem()
	}

	if !val.IsValid() || val.IsZero() {
		return true
	}

	return false
}
