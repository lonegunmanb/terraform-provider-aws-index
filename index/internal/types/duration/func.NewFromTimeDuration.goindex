package github.com/hashicorp/terraform-provider-aws/internal/types/duration
import (
	"errors"
	"fmt"
	"strconv"
	"strings"
	"time"

	"github.com/YakDriver/regexache"
)
func NewFromTimeDuration(t time.Duration) (result Duration) {
	u := uint64(t)

	const (
		day  = uint64(24 * time.Hour)
		year = 365 * day
	)

	if u >= year {
		result.years = int(u / year)
		u = u % year
	}

	result.days = int(u / day)

	return result
}
