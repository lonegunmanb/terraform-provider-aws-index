func GlobalSingletonIdentity(opts ...IdentityOptsFunc) Identity {
	identity := Identity{
		IsGlobalResource: true,
		IsSingleton:      true,
		Attributes: []IdentityAttribute{
			{
				Name:     "account_id",
				Required: false,
			},
		},
	}

	for _, opt := range opts {
		opt(&identity)
	}

	return identity
}
