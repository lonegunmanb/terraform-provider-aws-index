package github.com/hashicorp/terraform-provider-aws/internal/slices
import (
	"iter"
	"slices"
)
func AppendUnique[S ~[]E, E comparable](s S, vs ...E) S {
	for _, v := range vs {
		var exists bool

		if slices.Contains(s, v) {
			exists = true
		}

		if !exists {
			s = append(s, v)
		}
	}

	return s
}
