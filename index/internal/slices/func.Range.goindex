package github.com/hashicorp/terraform-provider-aws/internal/slices
import (
	"iter"
	"slices"
)
func Range[T signed](start, stop, step T) []T {
	v := make([]T, 0)

	switch {
	case step > 0:
		if start >= stop {
			return nil
		}
		for i := start; i < stop; i += step {
			v = append(v, i)
		}
	case step < 0:
		if start <= stop {
			return nil
		}
		for i := start; i > stop; i += step {
			v = append(v, i)
		}
	default:
		return nil
	}

	return v
}
