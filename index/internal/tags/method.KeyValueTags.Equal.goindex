func (tags KeyValueTags) Equal(other KeyValueTags) bool {
	if tags == nil && other == nil {
		return true
	}

	if tags == nil || other == nil {
		return false
	}

	if len(tags) != len(other) {
		return false
	}

	for k, v := range tags {
		o, ok := other[k]
		if !ok {
			return false
		}

		if !v.Equal(o) {
			return false
		}
	}

	return true
}
