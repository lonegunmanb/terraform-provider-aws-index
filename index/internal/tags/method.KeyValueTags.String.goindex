func (tags KeyValueTags) String() string {
	var builder strings.Builder

	keys := tags.Keys()
	slices.Sort(keys)

	builder.WriteString("map[")
	for i, k := range keys {
		if i > 0 {
			builder.WriteString(" ")
		}
		fmt.Fprintf(&builder, "%s:%s", k, tags[k].String())
	}
	builder.WriteString("]")

	return builder.String()
}
