func (tags KeyValueTags) IgnoreAWS() KeyValueTags { // nosemgrep:ci.aws-in-func-name
	result := make(KeyValueTags)

	for k, v := range tags {
		if !strings.HasPrefix(k, awsTagKeyPrefix) {
			result[k] = v
		}
	}

	return result
}
