func ValidOnceAWeekWindowFormat(v any, k string) (ws []string, errors []error) {
	value := v.(string)

	t := timestamp.New(value)
	if err := t.ValidateOnceAWeekWindowFormat(); err != nil {
		errors = append(errors, err)
		return
	}

	return
}
