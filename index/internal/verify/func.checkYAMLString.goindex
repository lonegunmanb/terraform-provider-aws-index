package github.com/hashicorp/terraform-provider-aws/internal/verify
import (
	"gopkg.in/yaml.v3"
)
func checkYAMLString(yamlString any) (string, error) {
	var y any

	if yamlString == nil || yamlString.(string) == "" {
		return "", nil
	}

	s := yamlString.(string)

	err := yaml.Unmarshal([]byte(s), &y)

	return s, err
}
