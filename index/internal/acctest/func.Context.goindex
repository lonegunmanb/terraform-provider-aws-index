package github.com/hashicorp/terraform-provider-aws/internal/acctest
import (
	"context"
	"testing"

	baselogging "github.com/hashicorp/aws-sdk-go-base/v2/logging"
	"github.com/hashicorp/terraform-plugin-log/tflog"
	"github.com/hashicorp/terraform-plugin-log/tfsdklog"
	helperlogging "github.com/hashicorp/terraform-plugin-sdk/v2/helper/logging"
)
func Context(t *testing.T) context.Context {
	t.Helper()

	helperlogging.SetOutput(t)

	ctx := t.Context()
	ctx = tfsdklog.ContextWithTestLogging(ctx, t.Name())
	ctx = logger(ctx, t, "acctest")
	ctx = awsSDKLogger(ctx)

	return ctx
}
