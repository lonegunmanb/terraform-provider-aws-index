package github.com/hashicorp/terraform-provider-aws/internal/service/s3control
import (
	"context"

	"github.com/aws/aws-sdk-go-v2/aws"
	awstypes "github.com/aws/aws-sdk-go-v2/service/s3control/types"
	tftags "github.com/hashicorp/terraform-provider-aws/internal/tags"
	"github.com/hashicorp/terraform-provider-aws/internal/types/option"
)
func getTagsInS3(ctx context.Context) []awstypes.S3Tag {
	if inContext, ok := tftags.FromContext(ctx); ok {
		if tags := tagsS3(inContext.TagsIn.UnwrapOrDefault()); len(tags) > 0 {
			return tags
		}
	}

	return nil
}
