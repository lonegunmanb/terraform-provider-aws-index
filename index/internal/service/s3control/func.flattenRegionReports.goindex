func flattenRegionReports(apiObjects []types.RegionReport) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenRegionReport(apiObject))
	}

	return tfList
}
