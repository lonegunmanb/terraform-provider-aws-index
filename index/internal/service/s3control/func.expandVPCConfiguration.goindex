func expandVPCConfiguration(tfMap map[string]any) *types.VpcConfiguration {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.VpcConfiguration{}

	if v, ok := tfMap[names.AttrVPCID].(string); ok {
		apiObject.VpcId = aws.String(v)
	}

	return apiObject
}
