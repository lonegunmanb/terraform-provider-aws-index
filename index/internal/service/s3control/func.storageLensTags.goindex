func storageLensTags(tags tftags.KeyValueTags) []types.StorageLensTag {
	result := make([]types.StorageLensTag, 0, len(tags))

	for k, v := range tags.Map() {
		tag := types.StorageLensTag{
			Key:   aws.String(k),
			Value: aws.String(v),
		}

		result = append(result, tag)
	}

	return result
}
