func expandRegions(tfList []any) []types.Region {
	if len(tfList) == 0 {
		return nil
	}

	var apiObjects []types.Region

	for _, tfMapRaw := range tfList {
		tfMap, ok := tfMapRaw.(map[string]any)

		if !ok {
			continue
		}

		apiObject := expandRegion(tfMap)

		if apiObject == nil {
			continue
		}

		apiObjects = append(apiObjects, *apiObject)
	}

	return apiObjects
}
