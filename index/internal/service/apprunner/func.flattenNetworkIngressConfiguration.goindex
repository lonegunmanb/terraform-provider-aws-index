func flattenNetworkIngressConfiguration(config *types.IngressConfiguration) []any {
	if config == nil {
		return []any{}
	}

	m := map[string]any{
		"is_publicly_accessible": config.IsPubliclyAccessible,
	}

	return []any{m}
}
