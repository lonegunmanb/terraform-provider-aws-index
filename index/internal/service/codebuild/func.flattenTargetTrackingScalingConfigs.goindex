func flattenTargetTrackingScalingConfigs(apiObjects []types.TargetTrackingScalingConfiguration) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenTargetTrackingScalingConfig(&apiObject))
	}

	return tfList
}
