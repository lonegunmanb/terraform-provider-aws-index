func flattenInputDevice(apiObject types.InputDeviceSettings) map[string]any {
	if apiObject == (types.InputDeviceSettings{}) {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.Id; v != nil {
		m[names.AttrID] = aws.ToString(v)
	}

	return m
}
