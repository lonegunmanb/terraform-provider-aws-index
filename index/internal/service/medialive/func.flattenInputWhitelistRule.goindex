func flattenInputWhitelistRule(apiObject types.InputWhitelistRule) map[string]any {
	if apiObject == (types.InputWhitelistRule{}) {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.Cidr; v != nil {
		m["cidr"] = aws.ToString(v)
	}

	return m
}
