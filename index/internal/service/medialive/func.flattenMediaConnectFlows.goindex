func flattenMediaConnectFlows(apiObjects []types.MediaConnectFlow) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var l []any

	for _, apiObject := range apiObjects {
		if apiObject == (types.MediaConnectFlow{}) {
			continue
		}

		l = append(l, flattenMediaConnectFlow(apiObject))
	}

	return l
}
