func flattenMediaConnectFlow(apiObject types.MediaConnectFlow) map[string]any {
	if apiObject == (types.MediaConnectFlow{}) {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.FlowArn; v != nil {
		m["flow_arn"] = aws.ToString(v)
	}

	return m
}
