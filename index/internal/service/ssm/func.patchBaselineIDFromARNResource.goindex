func patchBaselineIDFromARNResource(s string) string {
	re := regexache.MustCompile(`^patchbaseline/(` + patchBaselineIDRegexPattern + ")$")
	matches := re.FindStringSubmatch(s)
	if matches == nil || len(matches) != 2 {
		return ""
	}

	return matches[1]
}
