func flattenAliasRoutingConfiguration(apiObjects []awstypes.RoutingConfigurationListItem) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenAliasRoutingConfigurationItem(apiObject))
	}

	return tfList
}
