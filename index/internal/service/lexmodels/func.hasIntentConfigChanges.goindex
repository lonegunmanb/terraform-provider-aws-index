func hasIntentConfigChanges(d sdkv2.ResourceDiffer) bool {
	return slices.ContainsFunc([]string{
		names.AttrDescription,
		"conclusion_statement",
		"confirmation_prompt",
		"dialog_code_hook",
		"follow_up_prompt",
		"fulfillment_activity",
		"parent_intent_signature",
		"rejection_statement",
		"sample_utterances",
		"slot",
	}, d.HasChange)
}
