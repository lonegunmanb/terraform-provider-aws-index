func flattenResourceGroupTags(apiObjects []awstypes.ResourceGroupTag) map[string]any {
	tfMap := map[string]any{}

	for _, apiObject := range apiObjects {
		tfMap[aws.ToString(apiObject.Key)] = aws.ToString(apiObject.Value)
	}

	return tfMap
}
