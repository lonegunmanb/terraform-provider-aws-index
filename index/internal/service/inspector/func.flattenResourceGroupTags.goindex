func flattenResourceGroupTags(tags []awstypes.ResourceGroupTag) map[string]any {
	m := map[string]any{}

	for _, tag := range tags {
		m[aws.ToString(tag.Key)] = aws.ToString(tag.Value)
	}

	return m
}
