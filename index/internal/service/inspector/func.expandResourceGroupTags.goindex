func expandResourceGroupTags(m map[string]any) []awstypes.ResourceGroupTag {
	var result []awstypes.ResourceGroupTag

	for k, v := range m {
		result = append(result, awstypes.ResourceGroupTag{
			Key:   aws.String(k),
			Value: aws.String(v.(string)),
		})
	}

	return result
}
