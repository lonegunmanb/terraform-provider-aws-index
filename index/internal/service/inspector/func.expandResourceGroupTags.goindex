func expandResourceGroupTags(tfMap map[string]any) []awstypes.ResourceGroupTag {
	var apiObjects []awstypes.ResourceGroupTag

	for k, v := range tfMap {
		apiObjects = append(apiObjects, awstypes.ResourceGroupTag{
			Key:   aws.String(k),
			Value: aws.String(v.(string)),
		})
	}

	return apiObjects
}
