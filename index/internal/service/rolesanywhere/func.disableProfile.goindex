func disableProfile(ctx context.Context, conn *rolesanywhere.Client, id string) error {
	input := rolesanywhere.DisableProfileInput{
		ProfileId: aws.String(id),
	}

	_, err := conn.DisableProfile(ctx, &input)
	return err
}
