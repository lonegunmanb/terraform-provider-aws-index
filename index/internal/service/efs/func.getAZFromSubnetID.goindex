func getAZFromSubnetID(ctx context.Context, conn *ec2.Client, subnetID string) (string, error) {
	subnet, err := tfec2.FindSubnetByID(ctx, conn, subnetID)

	if err != nil {
		return "", err
	}

	return aws.ToString(subnet.AvailabilityZone), nil
}
