func expandBackupPolicy(tfMap map[string]any) *awstypes.BackupPolicy {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.BackupPolicy{}

	if v, ok := tfMap[names.AttrStatus].(string); ok && v != "" {
		apiObject.Status = awstypes.Status(v)
	}

	return apiObject
}
