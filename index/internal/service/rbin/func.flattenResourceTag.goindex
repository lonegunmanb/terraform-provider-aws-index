func flattenResourceTag(rTag types.ResourceTag) map[string]any {
	m := map[string]any{}

	if v := rTag.ResourceTagKey; v != nil {
		m["resource_tag_key"] = aws.ToString(v)
	}

	if v := rTag.ResourceTagValue; v != nil {
		m["resource_tag_value"] = aws.ToString(v)
	}

	return m
}
