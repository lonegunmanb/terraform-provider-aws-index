func flattenLogs(logs *types.LogsSummary) []any {
	if logs == nil {
		return []any{}
	}

	m := map[string]any{}

	if logs.General != nil {
		m["general"] = aws.ToBool(logs.General)
	}

	if logs.Audit != nil {
		m["audit"] = strconv.FormatBool(aws.ToBool(logs.Audit))
	}

	return []any{m}
}
