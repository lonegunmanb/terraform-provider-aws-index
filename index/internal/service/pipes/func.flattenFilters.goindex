func flattenFilters(apiObjects []types.Filter) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenFilter(apiObject))
	}

	return tfList
}
