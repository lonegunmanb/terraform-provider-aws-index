func expandFilter(tfMap map[string]any) *types.Filter {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.Filter{}

	if v, ok := tfMap["pattern"].(string); ok && v != "" {
		apiObject.Pattern = aws.String(v)
	}

	return apiObject
}
