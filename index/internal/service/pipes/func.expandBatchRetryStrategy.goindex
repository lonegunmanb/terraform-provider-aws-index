func expandBatchRetryStrategy(tfMap map[string]any) *types.BatchRetryStrategy {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.BatchRetryStrategy{}

	if v, ok := tfMap["attempts"].(int); ok {
		apiObject.Attempts = aws.Int32(int32(v))
	}

	return apiObject
}
