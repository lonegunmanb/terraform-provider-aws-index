func expandDeadLetterConfig(tfMap map[string]any) *types.DeadLetterConfig {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.DeadLetterConfig{}

	if v, ok := tfMap[names.AttrARN].(string); ok && v != "" {
		apiObject.Arn = aws.String(v)
	}

	return apiObject
}
