func flattenFilterCriteria(apiObject *types.FilterCriteria) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.Filters; v != nil {
		tfMap[names.AttrFilter] = flattenFilters(v)
	}

	return tfMap
}
