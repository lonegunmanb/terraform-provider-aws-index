func expandFilterCriteria(tfMap map[string]any) *types.FilterCriteria {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.FilterCriteria{}

	if v, ok := tfMap[names.AttrFilter].([]any); ok && len(v) > 0 {
		apiObject.Filters = expandFilters(v)
	}

	return apiObject
}
