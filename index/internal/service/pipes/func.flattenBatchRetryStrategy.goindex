func flattenBatchRetryStrategy(apiObject *types.BatchRetryStrategy) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.Attempts; v != nil {
		tfMap["attempts"] = aws.ToInt32(v)
	}

	return tfMap
}
