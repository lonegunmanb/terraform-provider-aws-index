func flattenECSEphemeralStorage(apiObject *types.EcsEphemeralStorage) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"size_in_gib": apiObject.SizeInGiB,
	}

	return tfMap
}
