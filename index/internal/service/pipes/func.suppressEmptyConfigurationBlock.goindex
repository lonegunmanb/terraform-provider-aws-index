func suppressEmptyConfigurationBlock(key string) schema.SchemaDiffSuppressFunc {
	return func(k, o, n string, d *schema.ResourceData) bool {
		if k != key+".#" {
			return false
		}

		if o == "0" && n == "1" {
			v := d.Get(key).([]any)
			return len(v) == 0 || v[0] == nil || len(v[0].(map[string]any)) == 0
		}

		return false
	}
}
