func findJobTemplateByID(ctx context.Context, conn *emrcontainers.Client, id string) (*awstypes.JobTemplate, error) {
	input := &emrcontainers.DescribeJobTemplateInput{
		Id: aws.String(id),
	}

	output, err := findJobTemplate(ctx, conn, input)

	if err != nil {
		return nil, err
	}

	return output, nil
}
