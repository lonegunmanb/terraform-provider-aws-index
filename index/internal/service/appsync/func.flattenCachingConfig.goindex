func flattenCachingConfig(apiObject *awstypes.CachingConfig) []any {
	if apiObject == nil {
		return nil
	}

	if len(apiObject.CachingKeys) == 0 && apiObject.Ttl == 0 {
		return nil
	}

	tfMap := map[string]any{
		"caching_keys": apiObject.CachingKeys,
		"ttl":          apiObject.Ttl,
	}

	return []any{tfMap}
}
