func flattenPipelineConfig(apiObject *awstypes.PipelineConfig) []any {
	if apiObject == nil {
		return nil
	}

	if len(apiObject.Functions) == 0 {
		return nil
	}

	tfMap := map[string]any{
		"functions": apiObject.Functions,
	}

	return []any{tfMap}
}
