func bucketCreateTags(ctx context.Context, conn *s3.Client, identifier string, tags []awstypes.Tag) error {
	if len(tags) == 0 {
		return nil
	}

	return bucketUpdateTags(ctx, conn, identifier, nil, keyValueTags(ctx, tags))
}
