func flattenEncryptionConfiguration(apiObject *types.EncryptionConfiguration) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := make(map[string]any)

	if apiObject.ReplicaKmsKeyID != nil {
		tfMap["replica_kms_key_id"] = aws.ToString(apiObject.ReplicaKmsKeyID)
	}

	return []any{tfMap}
}
