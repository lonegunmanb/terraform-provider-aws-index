func keyInXMLCharacterRange(key string) bool {
	for _, r := range key {
		if !(r == 0x09 ||
			r == 0x0A ||
			r == 0x0D ||
			r >= 0x20 && r <= 0xD7FF ||
			r >= 0xE000 && r <= 0xFFFD ||
			r >= 0x10000 && r <= 0x10FFFF) {
			return false
		}
	}
	return true
}
