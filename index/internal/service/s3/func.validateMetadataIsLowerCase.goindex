func validateMetadataIsLowerCase(v any, k string) (ws []string, errors []error) {
	value := v.(map[string]any)

	for k := range value {
		if k != strings.ToLower(k) {
			errors = append(errors, fmt.Errorf(
				"Metadata must be lowercase only. Offending key: %q", k))
		}
	}
	return
}
