func flattenRedirectAllRequestsTo(r *types.RedirectAllRequestsTo) []any {
	if r == nil {
		return []any{}
	}

	m := map[string]any{
		names.AttrProtocol: string(r.Protocol),
	}

	if r.HostName != nil {
		m["host_name"] = aws.ToString(r.HostName)
	}

	return []any{m}
}
