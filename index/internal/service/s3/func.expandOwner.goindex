func expandOwner(l []any) *types.Owner {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	tfMap, ok := l[0].(map[string]any)
	if !ok {
		return nil
	}

	owner := &types.Owner{}

	if v, ok := tfMap[names.AttrDisplayName].(string); ok && v != "" {
		owner.DisplayName = aws.String(v)
	}

	if v, ok := tfMap[names.AttrID].(string); ok && v != "" {
		owner.ID = aws.String(v)
	}

	return owner
}
