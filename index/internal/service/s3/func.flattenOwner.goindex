func flattenOwner(owner *types.Owner) []any {
	if owner == nil {
		return []any{}
	}

	m := make(map[string]any)

	if owner.DisplayName != nil {
		m[names.AttrDisplayName] = aws.ToString(owner.DisplayName)
	}

	if owner.ID != nil {
		m[names.AttrID] = aws.ToString(owner.ID)
	}

	return []any{m}
}
