func flattenIndexDocument(i *types.IndexDocument) []any {
	if i == nil {
		return []any{}
	}

	m := make(map[string]any)

	if i.Suffix != nil {
		m["suffix"] = aws.ToString(i.Suffix)
	}

	return []any{m}
}
