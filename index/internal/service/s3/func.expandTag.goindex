func expandTag(tfMap map[string]any) *types.Tag {
	if len(tfMap) == 0 {
		return nil
	}

	apiObject := &types.Tag{}

	if v, ok := tfMap[names.AttrKey].(string); ok {
		apiObject.Key = aws.String(v)
	}

	if v, ok := tfMap[names.AttrValue].(string); ok {
		apiObject.Value = aws.String(v)
	}

	return apiObject
}
