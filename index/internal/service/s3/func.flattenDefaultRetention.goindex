func flattenDefaultRetention(dr *types.DefaultRetention) []any {
	if dr == nil {
		return []any{}
	}

	m := map[string]any{
		"days":         dr.Days,
		names.AttrMode: dr.Mode,
		"years":        dr.Years,
	}

	return []any{m}
}
