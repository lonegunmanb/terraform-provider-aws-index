func flattenGrants(grants []types.Grant) []any {
	var results []any

	for _, grant := range grants {
		m := map[string]any{
			"permission": grant.Permission,
		}

		if grant.Grantee != nil {
			m["grantee"] = flattenACLGrantee(grant.Grantee)
		}

		results = append(results, m)
	}

	return results
}
