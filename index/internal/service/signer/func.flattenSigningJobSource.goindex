func flattenSigningJobSource(apiObject *types.Source) []any {
	if apiObject == nil || apiObject.S3 == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"s3": flattenSigningJobS3Source(apiObject.S3),
	}

	return []any{tfMap}
}
