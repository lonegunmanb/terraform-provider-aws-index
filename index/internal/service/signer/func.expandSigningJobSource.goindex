func expandSigningJobSource(tfList []any) *types.Source {
	if tfList == nil || tfList[0] == nil {
		return nil
	}

	tfMap, ok := tfList[0].(map[string]any)
	if !ok {
		return nil
	}

	var source *types.Source
	if v, ok := tfMap["s3"].([]any); ok && len(v) > 0 {
		source = &types.Source{
			S3: expandSigningJobS3Source(v),
		}
	}

	return source
}
