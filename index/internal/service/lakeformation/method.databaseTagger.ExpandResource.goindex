func (t *databaseTagger) ExpandResource(d *schema.ResourceData) *awstypes.Resource {
	v := d.Get(names.AttrDatabase).([]any)[0].(map[string]any)
	return &awstypes.Resource{
		Database: ExpandDatabaseResource(v),
	}
}
