func userAttributeKeyMatchesStandardAttribute(k string) bool {
	standardAttributeKeys := []string{
		names.AttrAddress,
		"birthdate",
		names.AttrEmail,
		"email_verified",
		"gender",
		"given_name",
		"family_name",
		"locale",
		"middle_name",
		names.AttrName,
		"nickname",
		"phone_number",
		"phone_number_verified",
		"picture",
		"preferred_username",
		names.AttrProfile,
		"sub",
		"updated_at",
		"website",
		"zoneinfo",
	}

	return slices.Contains(standardAttributeKeys, k)
}
