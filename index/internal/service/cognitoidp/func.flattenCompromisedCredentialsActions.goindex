func flattenCompromisedCredentialsActions(apiObject *awstypes.CompromisedCredentialsActionsType) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"event_action": apiObject.EventAction,
	}

	return []any{tfMap}
}
