func flattenSignInPolicyType(apiObject *awstypes.SignInPolicyType) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"allowed_first_auth_factors": apiObject.AllowedFirstAuthFactors,
	}

	if len(tfMap) > 0 {
		return []any{tfMap}
	}

	return []any{}
}
