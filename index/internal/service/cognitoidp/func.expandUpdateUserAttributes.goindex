func expandUpdateUserAttributes(oldMap, newMap map[string]any) (map[string]any, []string) {
	upd := make(map[string]any)

	for k, v := range newMap {
		if old, ok := oldMap[k]; ok {
			if old.(string) != v.(string) {
				upd[k] = v
			}
			delete(oldMap, k)
		} else {
			upd[k] = v
		}
	}

	del := tfmaps.Keys(oldMap)

	return upd, del
}
