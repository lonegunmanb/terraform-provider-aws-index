func flattenIndexFieldStatuses(apiObjects []types.IndexFieldStatus) ([]any, error) {
	if len(apiObjects) == 0 {
		return nil, nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfMap, err := flattenIndexFieldStatus(apiObject)

		if err != nil {
			return nil, err
		}

		tfList = append(tfList, tfMap)
	}

	return tfList, nil
}
