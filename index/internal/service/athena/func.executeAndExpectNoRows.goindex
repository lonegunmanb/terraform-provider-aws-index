func executeAndExpectNoRows(ctx context.Context, conn *athena.Client, qeid string) error {
	rs, err := queryExecutionResult(ctx, conn, qeid)
	if err != nil {
		return err
	}
	if len(rs.Rows) != 0 {
		return fmt.Errorf("unexpected query result: %s", flattenResultSet(rs))
	}
	return nil
}
