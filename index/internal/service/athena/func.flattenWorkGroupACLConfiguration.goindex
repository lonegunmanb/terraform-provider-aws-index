func flattenWorkGroupACLConfiguration(aclConfig *types.AclConfiguration) []any {
	if aclConfig == nil {
		return []any{}
	}

	m := map[string]any{
		"s3_acl_option": aclConfig.S3AclOption,
	}

	return []any{m}
}
