func flattenSteps(apiObjects []awstypes.ScheduledSplit) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		if apiObject.StartTime == nil {
			continue
		}

		tfList = append(tfList, flattenStep(apiObject))
	}

	return tfList
}
