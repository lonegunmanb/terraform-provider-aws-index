func flattenIndexingFilter(apiObject *awstypes.IndexingFilter) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.NamedShadowNames; v != nil {
		tfMap["named_shadow_names"] = aws.StringSlice(v)
	}

	return tfMap
}
