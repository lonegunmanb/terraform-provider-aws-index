func flattenFields(apiObjects []awstypes.Field) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenField(apiObject))
	}

	return tfList
}
