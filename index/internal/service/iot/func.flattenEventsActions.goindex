func flattenEventsActions(apiObjects []awstypes.Action) []any {
	tfList := make([]any, 0)

	for _, apiObject := range apiObjects {
		if v := apiObject.IotEvents; v != nil {
			tfList = append(tfList, flattenEventsAction(v)...)
		}
	}

	return tfList
}
