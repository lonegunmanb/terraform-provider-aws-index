func flattenTlsConfig(apiObject *awstypes.TlsConfig) map[string]any { // nosemgrep:ci.caps5-in-func-name
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.SecurityPolicy; v != nil {
		tfMap["security_policy"] = aws.ToString(v)
	}

	return tfMap
}
