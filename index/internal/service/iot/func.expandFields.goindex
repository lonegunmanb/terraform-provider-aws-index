func expandFields(tfList []any) []awstypes.Field {
	if len(tfList) == 0 {
		return nil
	}

	var apiObjects []awstypes.Field

	for _, tfMapRaw := range tfList {
		tfMap, ok := tfMapRaw.(map[string]any)

		if !ok {
			continue
		}

		apiObject := expandField(tfMap)

		if apiObject == nil {
			continue
		}

		apiObjects = append(apiObjects, *apiObject)
	}

	return apiObjects
}
