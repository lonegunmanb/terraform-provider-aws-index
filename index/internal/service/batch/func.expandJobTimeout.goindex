func expandJobTimeout(tfMap map[string]any) *awstypes.JobTimeout {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.JobTimeout{}

	if v, ok := tfMap["attempt_duration_seconds"].(int); ok && v != 0 {
		apiObject.AttemptDurationSeconds = aws.Int32(int32(v))
	}

	return apiObject
}
