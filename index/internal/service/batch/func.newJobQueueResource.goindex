func newJobQueueResource(_ context.Context) (resource.ResourceWithConfigure, error) {
	r := jobQueueResource{}

	r.SetDefaultCreateTimeout(10 * time.Minute)
	r.SetDefaultUpdateTimeout(10 * time.Minute)
	r.SetDefaultDeleteTimeout(10 * time.Minute)

	return &r, nil
}
