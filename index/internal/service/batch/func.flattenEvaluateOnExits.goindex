func flattenEvaluateOnExits(apiObjects []awstypes.EvaluateOnExit) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenEvaluateOnExit(&apiObject))
	}

	return tfList
}
