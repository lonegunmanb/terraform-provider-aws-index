func flattenMetricTransformations(apiObjects []awstypes.MetricTransformation) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenMetricTransformation(apiObject))
	}

	return tfList
}
