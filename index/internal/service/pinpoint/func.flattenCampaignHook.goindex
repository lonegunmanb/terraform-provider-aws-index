func flattenCampaignHook(ch *awstypes.CampaignHook) []any {
	l := make([]any, 0)

	m := map[string]any{}

	m["lambda_function_name"] = aws.ToString(ch.LambdaFunctionName)
	m[names.AttrMode] = ch.Mode
	m["web_url"] = aws.ToString(ch.WebUrl)

	l = append(l, m)

	return l
}
