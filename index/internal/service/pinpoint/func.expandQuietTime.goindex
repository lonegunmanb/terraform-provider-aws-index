func expandQuietTime(configs []any) *awstypes.QuietTime {
	if len(configs) == 0 || configs[0] == nil {
		return nil
	}

	m := configs[0].(map[string]any)

	qt := awstypes.QuietTime{}

	if v, ok := m["end"]; ok {
		qt.End = aws.String(v.(string))
	}

	if v, ok := m["start"]; ok {
		qt.Start = aws.String(v.(string))
	}

	return &qt
}
