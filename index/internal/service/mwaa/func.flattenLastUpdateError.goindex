func flattenLastUpdateError(apiObject *awstypes.UpdateError) []any {
	if apiObject == nil {
		return []any{}
	}

	m := map[string]any{}

	if apiObject.ErrorCode != nil {
		m["error_code"] = apiObject.ErrorCode
	}

	if apiObject.ErrorMessage != nil {
		m["error_message"] = apiObject.ErrorMessage
	}

	return []any{m}
}
