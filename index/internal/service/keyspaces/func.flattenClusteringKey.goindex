func flattenClusteringKey(apiObject *types.ClusteringKey) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"order_by": apiObject.OrderBy,
	}

	if v := apiObject.Name; v != nil {
		tfMap[names.AttrName] = aws.ToString(v)
	}

	return tfMap
}
