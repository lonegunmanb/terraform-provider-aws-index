func decodeOpenAPIDefinition(v string) (map[string]any, error) {
	var output map[string]any

	err := tfjson.DecodeFromString(v, &output)

	if err != nil {
		err = tfyaml.DecodeFromString(v, &output)
	}

	if err != nil {
		return nil, err
	}

	return output, nil
}
