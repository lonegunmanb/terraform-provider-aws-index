func getStackFailureEvents(ctx context.Context, conn *cloudformation.Client, name, requestToken string) []awstypes.StackEvent {
	events, err := findStackEventsForOperation(ctx, conn, name, requestToken, isFailedEvent)

	if err != nil {
		return nil
	}

	return events
}
