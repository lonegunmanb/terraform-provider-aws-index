func flattenLexBot(apiObject *awstypes.LexBot) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"lex_region":   aws.ToString(apiObject.LexRegion),
		names.AttrName: aws.ToString(apiObject.Name),
	}

	return []any{tfMap}
}
