func expandHierarchyLevelUpdate(tfList []any) *awstypes.HierarchyLevelUpdate {
	if len(tfList) == 0 {
		return nil
	}

	tfMap, ok := tfList[0].(map[string]any)
	if !ok {
		return nil
	}

	apiObject := &awstypes.HierarchyLevelUpdate{
		Name: aws.String(tfMap[names.AttrName].(string)),
	}

	return apiObject
}
