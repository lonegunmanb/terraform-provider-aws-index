func flattenKinesisFirehoseConfig(apiObject *awstypes.KinesisFirehoseConfig) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"firehose_arn": aws.ToString(apiObject.FirehoseArn),
	}

	return []any{tfMap}
}
