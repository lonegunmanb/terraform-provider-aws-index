func flattenPhoneNumberStatus(apiObject *awstypes.PhoneNumberStatus) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		names.AttrMessage: aws.ToString(apiObject.Message),
		names.AttrStatus:  apiObject.Status,
	}

	return []any{tfMap}
}
