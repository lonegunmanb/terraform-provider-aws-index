func expandLexBot(tfMap map[string]any) *awstypes.LexBot {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.LexBot{
		Name: aws.String(tfMap[names.AttrName].(string)),
	}

	if v, ok := tfMap["lex_region"].(string); ok && v != "" {
		apiObject.LexRegion = aws.String(v)
	}

	return apiObject
}
