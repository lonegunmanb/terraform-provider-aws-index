func channelsWithCrossChannelBehavior(cfgList []any) []string {
	var c []string
	for _, l := range cfgList {
		m := l.(map[string]any)
		if m == nil {
			continue
		}

		if v, ok := m["cross_channel_behavior"].([]any); ok && len(v) > 0 {
			c = append(c, m["channel"].(string))
		}
	}

	return c
}
