func flattenPortSets(apiObject map[string]awstypes.PortSet) []any {
	if apiObject == nil {
		return []any{}
	}

	tfList := make([]any, 0, len(apiObject))

	for k, v := range apiObject {
		tfMap := map[string]any{
			names.AttrKey: k,
			"port_set":    flattenPortSet(&v),
		}

		tfList = append(tfList, tfMap)
	}

	return tfList
}
