func flattenPublishMetricAction(apiObject *awstypes.PublishMetricAction) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"dimension": flattenDimensions(apiObject.Dimensions),
	}

	return []any{tfMap}
}
