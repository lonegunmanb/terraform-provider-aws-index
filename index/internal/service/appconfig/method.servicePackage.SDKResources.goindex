		{
			Factory:  resourceHostedConfigurationVersion,
			TypeName: "aws_appconfig_hosted_configuration_version",
			Name:     "Hosted Configuration Version",
			Region:   unique.Make(inttypes.ResourceRegionDefault()),
		},
