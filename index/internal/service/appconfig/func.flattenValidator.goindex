func flattenValidator(apiObject awstypes.Validator) map[string]any {
	tfMap := map[string]any{}

	if v := apiObject.Content; v != nil {
		tfMap[names.AttrContent] = aws.ToString(v)
	}

	tfMap[names.AttrType] = string(apiObject.Type)

	return tfMap
}
