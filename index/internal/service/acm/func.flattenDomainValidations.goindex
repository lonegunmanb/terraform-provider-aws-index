func flattenDomainValidations(apiObjects []types.DomainValidation) ([]any, []string) {
	if len(apiObjects) == 0 {
		return nil, nil
	}

	var tfList []any
	var tfStrings []string

	for _, apiObject := range apiObjects {
		v1, v2 := flattenDomainValidation(apiObject)

		if len(v1) > 0 {
			tfList = append(tfList, v1)
		}
		if len(v2) > 0 {
			tfStrings = append(tfStrings, v2...)
		}
	}

	return tfList, tfStrings
}
