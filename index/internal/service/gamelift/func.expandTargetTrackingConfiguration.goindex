func expandTargetTrackingConfiguration(tfMap map[string]any) *awstypes.TargetTrackingConfiguration {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.TargetTrackingConfiguration{
		TargetValue: aws.Float64(tfMap["target_value"].(float64)),
	}

	return apiObject
}
