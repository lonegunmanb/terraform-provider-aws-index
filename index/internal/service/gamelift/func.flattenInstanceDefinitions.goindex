func flattenInstanceDefinitions(apiObjects []awstypes.InstanceDefinition) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenInstanceDefinition(&apiObject))
	}

	return tfList
}
