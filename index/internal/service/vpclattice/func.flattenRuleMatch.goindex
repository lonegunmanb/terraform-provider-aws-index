func flattenRuleMatch(apiObject types.RuleMatch) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := make(map[string]any)

	if v, ok := apiObject.(*types.RuleMatchMemberHttpMatch); ok {
		tfMap["http_match"] = []any{flattenHTTPMatch(&v.Value)}
	}

	return tfMap
}
