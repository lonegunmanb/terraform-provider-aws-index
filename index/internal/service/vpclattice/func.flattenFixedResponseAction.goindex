func flattenFixedResponseAction(apiObject *types.FixedResponseAction) []any {
	tfMap := map[string]any{}

	if v := apiObject.StatusCode; v != nil {
		tfMap[names.AttrStatusCode] = aws.ToInt32(v)
	}

	return []any{tfMap}
}
