func findIPGroupByID(ctx context.Context, conn *workspaces.Client, id string) (*types.WorkspacesIpGroup, error) {
	input := &workspaces.DescribeIpGroupsInput{
		GroupIds: []string{id},
	}

	return findIPGroup(ctx, conn, input)
}
