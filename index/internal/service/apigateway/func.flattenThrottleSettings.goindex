func flattenThrottleSettings(s *types.ThrottleSettings) []map[string]any {
	settings := make(map[string]any)

	if s == nil {
		return nil
	}

	settings["burst_limit"] = s.BurstLimit
	settings["rate_limit"] = s.RateLimit

	return []map[string]any{settings}
}
