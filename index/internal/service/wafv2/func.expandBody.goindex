func expandBody(l []any) *awstypes.Body {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	body := &awstypes.Body{}

	if v, ok := m["oversize_handling"].(string); ok && v != "" {
		body.OversizeHandling = awstypes.OversizeHandling(v)
	}

	return body
}
