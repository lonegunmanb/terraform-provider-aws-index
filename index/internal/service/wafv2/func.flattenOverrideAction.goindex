func flattenOverrideAction(a *awstypes.OverrideAction) any {
	if a == nil {
		return []any{}
	}

	m := map[string]any{}

	if a.Count != nil {
		m["count"] = make([]map[string]any, 1)
	}

	if a.None != nil {
		m["none"] = make([]map[string]any, 1)
	}

	return []any{m}
}
