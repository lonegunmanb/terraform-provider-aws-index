func flattenCookies(c *awstypes.Cookies) any {
	if c == nil {
		return []any{}
	}

	m := map[string]any{
		"match_scope":       c.MatchScope,
		"oversize_handling": c.OversizeHandling,
		"match_pattern":     flattenCookiesMatchPattern(c.MatchPattern),
	}

	return []any{m}
}
