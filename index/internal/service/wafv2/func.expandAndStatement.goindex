func expandAndStatement(l []any) *awstypes.AndStatement {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	return &awstypes.AndStatement{
		Statements: expandStatements(m["statement"].([]any)),
	}
}
