func flattenJSONMatchPattern(p *awstypes.JsonMatchPattern) []any {
	if p == nil {
		return []any{}
	}

	m := map[string]any{
		"included_paths": p.IncludedPaths,
	}

	if p.All != nil {
		m["all"] = make([]map[string]any, 1)
	}

	return []any{m}
}
