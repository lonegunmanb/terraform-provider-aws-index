func flattenFieldToProtect(apiObject *awstypes.FieldToProtect) any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"field_type": apiObject.FieldType,
	}

	if v := apiObject.FieldKeys; len(v) > 0 {
		tfMap["field_keys"] = v
	}

	return []any{tfMap}
}
