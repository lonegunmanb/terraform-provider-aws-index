func flattenForwardedIPConfig(f *awstypes.ForwardedIPConfig) any {
	if f == nil {
		return []any{}
	}

	m := map[string]any{
		"fallback_behavior": f.FallbackBehavior,
		"header_name":       aws.ToString(f.HeaderName),
	}

	return []any{m}
}
