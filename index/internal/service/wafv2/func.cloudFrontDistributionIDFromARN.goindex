func cloudFrontDistributionIDFromARN(arn string) (string, error) {
	parts := strings.Split(arn, "/")
	if len(parts) < 2 {
		return "", fmt.Errorf("invalid CloudFront distribution ARN format: %s", arn)
	}
	return parts[len(parts)-1], nil
}
