func flattenBlock(a *awstypes.BlockAction) []any {
	if a == nil {
		return []any{}
	}

	m := map[string]any{}

	if a.CustomResponse != nil {
		m["custom_response"] = flattenCustomResponse(a.CustomResponse)
	}

	return []any{m}
}
