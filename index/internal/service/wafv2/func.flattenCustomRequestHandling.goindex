func flattenCustomRequestHandling(c *awstypes.CustomRequestHandling) []any {
	if c == nil {
		return []any{}
	}

	m := map[string]any{
		"insert_header": flattenCustomHeaders(c.InsertHeaders),
	}

	return []any{m}
}
