func flattenRateLimitQueryArgument(apiObject *awstypes.RateLimitQueryArgument) []any {
	if apiObject == nil {
		return nil
	}
	return []any{
		map[string]any{
			names.AttrName:        aws.ToString(apiObject.Name),
			"text_transformation": flattenTextTransformations(apiObject.TextTransformations),
		},
	}
}
