func expandCountryCodes(l []any) []awstypes.CountryCode {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	countryCodes := make([]awstypes.CountryCode, 0)
	for _, countryCode := range l {
		if countryCode == nil {
			continue
		}

		if v, ok := countryCode.(string); ok {
			countryCodes = append(countryCodes, awstypes.CountryCode(v))
		}
	}

	return countryCodes
}
