func flattenIPSetReferenceStatement(i *awstypes.IPSetReferenceStatement) any {
	if i == nil {
		return []any{}
	}

	m := map[string]any{
		names.AttrARN:                aws.ToString(i.ARN),
		"ip_set_forwarded_ip_config": flattenIPSetForwardedIPConfig(i.IPSetForwardedIPConfig),
	}

	return []any{m}
}
