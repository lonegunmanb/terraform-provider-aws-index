func flattenTextTransformations(l []awstypes.TextTransformation) []any {
	out := make([]any, len(l))
	for i, t := range l {
		m := make(map[string]any)
		m[names.AttrPriority] = t.Priority
		m[names.AttrType] = t.Type
		out[i] = m
	}
	return out
}
