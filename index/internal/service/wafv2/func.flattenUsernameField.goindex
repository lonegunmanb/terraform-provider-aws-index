func flattenUsernameField(apiObject *awstypes.UsernameField) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{
		names.AttrIdentifier: aws.ToString(apiObject.Identifier),
	}

	return []any{m}
}
