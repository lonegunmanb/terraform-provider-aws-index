func expandRuleLabels(l []any) []awstypes.Label {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	labels := make([]awstypes.Label, 0)

	for _, label := range l {
		if label == nil {
			continue
		}
		m := label.(map[string]any)
		labels = append(labels, awstypes.Label{
			Name: aws.String(m[names.AttrName].(string)),
		})
	}

	return labels
}
