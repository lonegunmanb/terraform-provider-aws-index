func flattenStatusCode(apiObject *awstypes.ResponseInspectionStatusCode) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{
		"failure_codes": flex.FlattenInt32ValueSet(apiObject.FailureCodes),
		"success_codes": flex.FlattenInt32ValueSet(apiObject.SuccessCodes),
	}

	return []any{m}
}
