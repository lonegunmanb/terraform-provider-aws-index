func expandWebACLRules(l []any) []awstypes.Rule {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	rules := make([]awstypes.Rule, 0)

	for _, rule := range l {
		if rule == nil {
			continue
		}
		rules = append(rules, expandWebACLRule(rule.(map[string]any)))
	}

	return rules
}
