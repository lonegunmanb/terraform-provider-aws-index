func flattenURIFragment(apiObject *awstypes.UriFragment) any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.FallbackBehavior; v != "" {
		tfMap["fallback_behavior"] = v
	}
	return []any{tfMap}
}
