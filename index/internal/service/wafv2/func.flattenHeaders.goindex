func flattenHeaders(s *awstypes.Headers) any {
	if s == nil {
		return []any{}
	}

	m := map[string]any{
		"match_scope":       s.MatchScope,
		"match_pattern":     flattenHeaderMatchPattern(s.MatchPattern),
		"oversize_handling": s.OversizeHandling,
	}

	return []any{m}
}
