func flattenRuleLabels(l []awstypes.Label) []any {
	if len(l) == 0 {
		return nil
	}

	out := make([]any, len(l))
	for i, label := range l {
		out[i] = map[string]any{
			names.AttrName: aws.ToString(label.Name),
		}
	}

	return out
}
