func flattenXSSMatchTuples(ts []awstypes.XssMatchTuple) []any {
	out := make([]any, len(ts))
	for i, t := range ts {
		m := make(map[string]any)
		m["field_to_match"] = flattenFieldToMatch(t.FieldToMatch)
		m["text_transformation"] = t.TextTransformation
		out[i] = m
	}
	return out
}
