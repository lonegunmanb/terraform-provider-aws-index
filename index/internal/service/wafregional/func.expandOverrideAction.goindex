func expandOverrideAction(l []any) *awstypes.WafOverrideAction {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	return &awstypes.WafOverrideAction{
		Type: awstypes.WafOverrideActionType(m[names.AttrType].(string)),
	}
}
