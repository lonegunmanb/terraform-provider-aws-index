func flattenAlarms(apiObject *awstypes.DeploymentAlarms) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.AlarmNames; v != nil {
		tfMap["alarm_names"] = v
	}

	tfMap["enable"] = apiObject.Enable

	tfMap["rollback"] = apiObject.Rollback

	return tfMap
}
