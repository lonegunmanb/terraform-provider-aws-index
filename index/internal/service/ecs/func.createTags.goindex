func createTags(ctx context.Context, conn *ecs.Client, identifier string, tags []awstypes.Tag, optFns ...func(*ecs.Options)) error {
	if len(tags) == 0 {
		return nil
	}

	return updateTags(ctx, conn, identifier, nil, keyValueTags(ctx, tags), optFns...)
}
