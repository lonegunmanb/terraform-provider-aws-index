func flattenCanaryScheduleRetryConfig(retryConfig *awstypes.RetryConfigOutput) []any {
	if retryConfig == nil {
		return []any{}
	}
	m := map[string]any{
		"max_retries": aws.ToInt32(retryConfig.MaxRetries),
	}

	return []any{m}
}
