func findGraphByID(ctx context.Context, conn *neptunegraph.Client, id string) (*neptunegraph.GetGraphOutput, error) {
	input := neptunegraph.GetGraphInput{
		GraphIdentifier: aws.String(id),
	}

	return findGraph(ctx, conn, &input)
}
