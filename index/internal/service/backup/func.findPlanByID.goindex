func findPlanByID(ctx context.Context, conn *backup.Client, id string) (*backup.GetBackupPlanOutput, error) {
	input := &backup.GetBackupPlanInput{
		BackupPlanId: aws.String(id),
	}

	return findPlan(ctx, conn, input)
}
