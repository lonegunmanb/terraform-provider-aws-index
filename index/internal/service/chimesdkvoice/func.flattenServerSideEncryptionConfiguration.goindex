func flattenServerSideEncryptionConfiguration(apiObject *awstypes.ServerSideEncryptionConfiguration) []any {
	if apiObject == nil {
		return nil
	}

	return []any{map[string]any{
		names.AttrKMSKeyARN: apiObject.KmsKeyArn,
	}}
}
