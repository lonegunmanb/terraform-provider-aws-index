func flattenVPCEndpoints(apiObjects []awstypes.VpcEndpoint) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenVPCEndpoint(&apiObject))
	}

	return tfList
}
