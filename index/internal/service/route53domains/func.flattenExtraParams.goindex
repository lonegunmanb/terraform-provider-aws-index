func flattenExtraParams(apiObjects []awstypes.ExtraParam) map[string]any {
	if len(apiObjects) == 0 {
		return nil
	}

	tfMap := make(map[string]any, len(apiObjects))

	for _, apiObject := range apiObjects {
		tfMap[string(apiObject.Name)] = aws.ToString(apiObject.Value)
	}

	return tfMap
}
