func flattenNameserver(apiObject *awstypes.Nameserver) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.GlueIps; v != nil {
		tfMap["glue_ips"] = v
	}

	if v := apiObject.Name; v != nil {
		tfMap[names.AttrName] = aws.ToString(v)
	}

	return tfMap
}
