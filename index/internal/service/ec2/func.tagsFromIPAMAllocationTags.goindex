func tagsFromIPAMAllocationTags(rts []awstypes.IpamResourceTag) []awstypes.Tag {
	if len(rts) == 0 {
		return nil
	}

	tags := []awstypes.Tag{}
	for _, ts := range rts {
		tags = append(tags, awstypes.Tag{
			Key:   ts.Key,
			Value: ts.Value,
		})
	}

	return tags
}
