func flattenIPAMPoolCIDR(c awstypes.IpamPoolCidr) map[string]any {
	cidr := make(map[string]any)
	cidr["cidr"] = aws.ToString(c.Cidr)
	cidr[names.AttrState] = c.State
	return cidr
}
