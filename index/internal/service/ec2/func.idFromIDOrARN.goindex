func idFromIDOrARN(idOrARN string) string {
	// e.g. "eni-02ae120b80627a68f" or
	// "arn:aws:ec2:ap-southeast-2:123456789012:network-interface/eni-02ae120b80627a68f".
	return idOrARN[strings.LastIndex(idOrARN, "/")+1:]
}
