func flattenIPAMPoolCIDRs(c []awstypes.IpamPoolCidr) []any {
	cidrs := []any{}
	for _, cidr := range c {
		cidrs = append(cidrs, flattenIPAMPoolCIDR(cidr))
	}
	return cidrs
}
