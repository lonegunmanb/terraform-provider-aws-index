func (d *securityGroupRuleDataSource) securityGroupRuleARN(ctx context.Context, id string) types.String {
	return types.StringValue(d.Meta().RegionalARN(ctx, names.EC2, fmt.Sprintf("security-group-rule/%s", id)))
}
