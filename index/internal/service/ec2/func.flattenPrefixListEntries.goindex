func flattenPrefixListEntries(apiObjects []awstypes.PrefixListEntry) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenPrefixListEntry(apiObject))
	}

	return tfList
}
