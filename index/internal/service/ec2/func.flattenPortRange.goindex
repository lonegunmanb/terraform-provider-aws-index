func flattenPortRange(apiObject *awstypes.PortRange) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.From; v != nil {
		tfMap["from"] = aws.ToInt32(v)
	}

	if v := apiObject.To; v != nil {
		tfMap["to"] = aws.ToInt32(v)
	}

	return tfMap
}
