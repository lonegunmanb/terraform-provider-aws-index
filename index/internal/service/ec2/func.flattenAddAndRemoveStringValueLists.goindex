func flattenAddAndRemoveStringValueLists(d *schema.ResourceData, key string) ([]string, []string) {
	if !d.HasChange(key) {
		return nil, nil
	}

	var add, del []string

	o, n := d.GetChange(key)
	os := o.(*schema.Set)
	ns := n.(*schema.Set)

	if v := flex.ExpandStringValueSet(ns.Difference(os)); len(v) > 0 {
		add = v
	}

	if v := flex.ExpandStringValueSet(os.Difference(ns)); len(v) > 0 {
		del = v
	}

	return add, del
}
