func expandPrivateIPAddressSpecification(tfString string) *awstypes.PrivateIpAddressSpecification {
	if tfString == "" {
		return nil
	}

	apiObject := &awstypes.PrivateIpAddressSpecification{
		PrivateIpAddress: aws.String(tfString),
	}

	return apiObject
}
