func flattenVerifiedAccessEndpointPortRanges(apiObjects []awstypes.VerifiedAccessEndpointPortRange) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	tfList := make([]any, len(apiObjects))

	for i, apiObject := range apiObjects {
		tfMap := map[string]any{}

		if v := apiObject.FromPort; v != nil {
			tfMap["from_port"] = aws.ToInt32(v)
		}

		if v := apiObject.ToPort; v != nil {
			tfMap["to_port"] = aws.ToInt32(v)
		}

		tfList[i] = tfMap
	}

	return tfList
}
