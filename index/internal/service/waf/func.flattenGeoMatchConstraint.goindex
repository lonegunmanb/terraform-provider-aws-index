func flattenGeoMatchConstraint(ts []awstypes.GeoMatchConstraint) []any {
	out := make([]any, len(ts))
	for i, t := range ts {
		m := make(map[string]any)
		m[names.AttrType] = string(t.Type)
		m[names.AttrValue] = string(t.Value)
		out[i] = m
	}
	return out
}
