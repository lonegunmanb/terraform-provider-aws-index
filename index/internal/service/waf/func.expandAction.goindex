func expandAction(l []any) *awstypes.WafAction {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	return &awstypes.WafAction{
		Type: awstypes.WafActionType(m[names.AttrType].(string)),
	}
}
