func flattenMetricDimensions(ds []awstypes.MetricDimension) []any {
	l := make([]any, len(ds))
	for i, d := range ds {
		if ds == nil {
			continue
		}

		m := map[string]any{}

		if v := d.Name; v != nil {
			m[names.AttrName] = aws.ToString(v)
		}

		if v := d.Value; v != nil {
			m[names.AttrValue] = aws.ToString(v)
		}

		l[i] = m
	}
	return l
}
