func flattenMessageReviewHandler(apiObject *types.MessageReviewHandler) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.FallbackResult; v != "" {
		m["fallback_result"] = v
	}

	if v := apiObject.Uri; v != nil {
		m[names.AttrURI] = aws.ToString(v)
	}

	return []any{m}
}
