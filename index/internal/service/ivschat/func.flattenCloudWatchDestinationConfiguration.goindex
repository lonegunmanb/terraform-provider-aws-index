func flattenCloudWatchDestinationConfiguration(apiObject types.CloudWatchLogsDestinationConfiguration) []any {
	m := map[string]any{}

	if v := apiObject.LogGroupName; v != nil {
		m[names.AttrLogGroupName] = aws.ToString(v)
	}

	return []any{m}
}
