func flattenFirehoseDestinationConfiguration(apiObject types.FirehoseDestinationConfiguration) []any {
	m := map[string]any{}

	if v := apiObject.DeliveryStreamName; v != nil {
		m["delivery_stream_name"] = aws.ToString(v)
	}

	return []any{m}
}
