package github.com/hashicorp/terraform-provider-aws/internal/service/opensearch
import (
	"time"

	"github.com/aws/aws-sdk-go-v2/aws"
	awstypes "github.com/aws/aws-sdk-go-v2/service/opensearch/types"
	"github.com/hashicorp/terraform-plugin-sdk/v2/helper/schema"
	"github.com/hashicorp/terraform-provider-aws/names"
)
func flattenWindowStartTime(apiObject *awstypes.WindowStartTime) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"hours":   apiObject.Hours,
		"minutes": apiObject.Minutes,
	}

	return tfMap
}
