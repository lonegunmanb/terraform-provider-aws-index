func flattenNodeToNodeEncryptionOptions(o *awstypes.NodeToNodeEncryptionOptions) []map[string]any {
	if o == nil {
		return []map[string]any{}
	}

	m := map[string]any{}
	if o.Enabled != nil {
		m[names.AttrEnabled] = aws.ToBool(o.Enabled)
	}

	return []map[string]any{m}
}
