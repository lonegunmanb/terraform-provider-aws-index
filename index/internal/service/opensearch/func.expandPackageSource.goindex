func expandPackageSource(v any) *awstypes.PackageSource {
	if v == nil {
		return nil
	}

	return &awstypes.PackageSource{
		S3BucketName: aws.String(v.(map[string]any)[names.AttrS3BucketName].(string)),
		S3Key:        aws.String(v.(map[string]any)["s3_key"].(string)),
	}
}
