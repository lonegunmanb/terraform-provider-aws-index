func expandPortRange(tfMap map[string]any) *awstypes.PortRange {
	apiObject := &awstypes.PortRange{}

	apiObject.MinRange = aws.Int32(int32(tfMap["min_range"].(int)))
	apiObject.MaxRange = aws.Int32(int32(tfMap["max_range"].(int)))

	return apiObject
}
