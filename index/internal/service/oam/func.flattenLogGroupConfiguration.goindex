func flattenLogGroupConfiguration(apiObject *awstypes.LogGroupConfiguration) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{}

	if apiObject.Filter != nil {
		tfMap[names.AttrFilter] = aws.ToString(apiObject.Filter)
	}

	return []any{tfMap}
}
