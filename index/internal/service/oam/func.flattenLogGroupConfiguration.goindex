func flattenLogGroupConfiguration(a *types.LogGroupConfiguration) []any {
	if a == nil {
		return []any{}
	}
	m := map[string]any{}

	if a.Filter != nil {
		m[names.AttrFilter] = aws.ToString(a.Filter)
	}

	return []any{m}
}
