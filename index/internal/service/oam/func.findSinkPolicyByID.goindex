func findSinkPolicyByID(ctx context.Context, conn *oam.Client, id string) (*oam.GetSinkPolicyOutput, error) {
	in := oam.GetSinkPolicyInput{
		SinkIdentifier: aws.String(id),
	}

	return findSinkPolicy(ctx, conn, &in)
}
