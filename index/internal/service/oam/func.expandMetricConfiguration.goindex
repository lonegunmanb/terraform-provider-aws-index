func expandMetricConfiguration(l []any) *types.MetricConfiguration {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	config := &types.MetricConfiguration{}

	m := l[0].(map[string]any)
	if v, ok := m[names.AttrFilter]; ok && v != "" {
		config.Filter = aws.String(v.(string))
	}

	return config
}
