func expandTrafficRoute(l []any) *types.TrafficRoute {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	trafficRoute := &types.TrafficRoute{
		ListenerArns: flex.ExpandStringValueSet(m["listener_arns"].(*schema.Set)),
	}

	return trafficRoute
}
