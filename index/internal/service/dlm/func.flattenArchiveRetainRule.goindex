func flattenArchiveRetainRule(apiObject *awstypes.ArchiveRetainRule) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := make(map[string]any)
	tfMap["retention_archive_tier"] = flattenRetentionArchiveTier(apiObject.RetentionArchiveTier)

	return []any{tfMap}
}
