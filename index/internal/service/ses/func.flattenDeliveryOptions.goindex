func flattenDeliveryOptions(apiObject *awstypes.DeliveryOptions) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"tls_policy": string(apiObject.TlsPolicy),
	}

	return []any{tfMap}
}
