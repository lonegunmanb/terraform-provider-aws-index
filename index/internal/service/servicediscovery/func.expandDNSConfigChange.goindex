func expandDNSConfigChange(tfMap map[string]any) *awstypes.DnsConfigChange {
	if len(tfMap) == 0 {
		return nil
	}

	apiObject := &awstypes.DnsConfigChange{}

	if v, ok := tfMap["dns_records"].([]any); ok && len(v) > 0 {
		apiObject.DnsRecords = expandDNSRecords(v)
	}

	return apiObject
}
