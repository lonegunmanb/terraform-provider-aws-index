func flattenPercentileAggregation(apiObject *awstypes.PercentileAggregation) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if apiObject.PercentileValue != nil {
		tfMap["percentile_value"] = aws.ToFloat64(apiObject.PercentileValue)
	}

	return []any{tfMap}
}
