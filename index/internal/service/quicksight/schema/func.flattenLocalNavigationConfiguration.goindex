func flattenLocalNavigationConfiguration(apiObject *awstypes.LocalNavigationConfiguration) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"target_sheet_id": aws.ToString(apiObject.TargetSheetId),
	}

	return []any{tfMap}
}
