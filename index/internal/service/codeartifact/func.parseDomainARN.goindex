func parseDomainARN(v string) (string, string, error) {
	// arn:${Partition}:codeartifact:${Region}:${Account}:domain/${DomainName}
	arn, err := arn.Parse(v)
	if err != nil {
		return "", "", err
	}

	return arn.AccountID, strings.TrimPrefix(arn.Resource, "domain/"), nil
}
