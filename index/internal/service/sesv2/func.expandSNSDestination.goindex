func expandSNSDestination(tfMap map[string]any) *types.SnsDestination {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.SnsDestination{}

	if v, ok := tfMap[names.AttrTopicARN].(string); ok && v != "" {
		apiObject.TopicArn = aws.String(v)
	}

	return apiObject
}
