func expandReputationOptions(tfMap map[string]any) *types.ReputationOptions {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.ReputationOptions{}

	if v, ok := tfMap["reputation_metrics_enabled"].(bool); ok {
		apiObject.ReputationMetricsEnabled = v
	}

	return apiObject
}
