func (r *templateResource) ConfigValidators(context.Context) []resource.ConfigValidator {
	return []resource.ConfigValidator{
		resourcevalidator.ExactlyOneOf(
			path.MatchRoot("aws_region"),
			path.MatchRoot(names.AttrRegion),
		),
	}
}
