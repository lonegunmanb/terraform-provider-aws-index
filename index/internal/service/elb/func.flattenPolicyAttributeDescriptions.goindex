package github.com/hashicorp/terraform-provider-aws/internal/service/elb
import (
	"errors"
	"slices"
	"strings"

	"github.com/aws/aws-sdk-go-v2/aws"
	awstypes "github.com/aws/aws-sdk-go-v2/service/elasticloadbalancing/types"
	tfslices "github.com/hashicorp/terraform-provider-aws/internal/slices"
	"github.com/hashicorp/terraform-provider-aws/names"
)
func flattenPolicyAttributeDescriptions(apiObjects []awstypes.PolicyAttributeDescription) []any {
	var tfList []any

	for _, apiObject := range apiObjects {
		tfMap := map[string]string{
			names.AttrName:  aws.ToString(apiObject.AttributeName),
			names.AttrValue: aws.ToString(apiObject.AttributeValue),
		}

		tfList = append(tfList, tfMap)
	}

	return tfList
}
