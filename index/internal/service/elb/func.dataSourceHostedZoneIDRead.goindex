package github.com/hashicorp/terraform-provider-aws/internal/service/elb
import (
	"context"

	"github.com/hashicorp/aws-sdk-go-base/v2/endpoints"
	"github.com/hashicorp/terraform-plugin-sdk/v2/diag"
	"github.com/hashicorp/terraform-plugin-sdk/v2/helper/schema"
	"github.com/hashicorp/terraform-provider-aws/internal/conns"
	"github.com/hashicorp/terraform-provider-aws/internal/errs/sdkdiag"
)
func dataSourceHostedZoneIDRead(ctx context.Context, d *schema.ResourceData, meta any) diag.Diagnostics {
	var diags diag.Diagnostics

	region := meta.(*conns.AWSClient).Region(ctx)
	if v, ok := hostedZoneIDPerRegionMap[region]; ok {
		d.SetId(v)
		return diags
	}

	return sdkdiag.AppendErrorf(diags, "unsupported ELB Region (%s)", region)
}
