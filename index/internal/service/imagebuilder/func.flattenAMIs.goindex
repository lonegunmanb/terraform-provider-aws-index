func flattenAMIs(apiObjects []awstypes.Ami) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenAMI(apiObject))
	}

	return tfList
}
