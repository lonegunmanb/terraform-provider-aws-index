func flattenConsolidation(apiObject *types.Consolidation) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.MatchingAttributesList; v != nil {
		tfMap["matching_attributes_list"] = v
	}

	return []any{tfMap}
}
