func flattenExportingConfig(apiObject *types.ExportingConfig) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.S3Exporting; v != nil {
		tfMap["s3_exporting"] = flattenS3Exporting(v)
	}

	return []any{tfMap}
}
