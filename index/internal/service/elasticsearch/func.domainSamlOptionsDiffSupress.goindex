func domainSamlOptionsDiffSupress(k, old, new string, d *schema.ResourceData) bool {
	if v, ok := d.Get("saml_options").([]any); ok && len(v) > 0 {
		if enabled, ok := v[0].(map[string]any)[names.AttrEnabled].(bool); ok && !enabled {
			return true
		}
	}
	return false
}
