func expandSMBMountOptions(l []any) *awstypes.SmbMountOptions {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	smbMountOptions := &awstypes.SmbMountOptions{
		Version: awstypes.SmbVersion(m[names.AttrVersion].(string)),
	}

	return smbMountOptions
}
