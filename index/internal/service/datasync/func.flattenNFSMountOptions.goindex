func flattenNFSMountOptions(mountOptions *awstypes.NfsMountOptions) []any {
	if mountOptions == nil {
		return []any{}
	}

	m := map[string]any{
		names.AttrVersion: string(mountOptions.Version),
	}

	return []any{m}
}
