func flattenFilterRules(filterRules []awstypes.FilterRule) []any {
	l := []any{}

	for _, filterRule := range filterRules {
		m := map[string]any{
			"filter_type":   string(filterRule.FilterType),
			names.AttrValue: aws.ToString(filterRule.Value),
		}
		l = append(l, m)
	}

	return l
}
