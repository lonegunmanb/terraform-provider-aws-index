func flattenS3Config(s3Config *awstypes.S3Config) []any {
	if s3Config == nil {
		return []any{}
	}

	m := map[string]any{
		"bucket_access_role_arn": aws.ToString(s3Config.BucketAccessRoleArn),
	}

	return []any{m}
}
