func flattenHDFSQOPConfiguration(qopConfig *awstypes.QopConfiguration) []any {
	if qopConfig == nil {
		return []any{}
	}

	m := map[string]any{
		"data_transfer_protection": string(qopConfig.DataTransferProtection),
		"rpc_protection":           string(qopConfig.RpcProtection),
	}

	return []any{m}
}
