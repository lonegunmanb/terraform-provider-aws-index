func flattenEC2Config(ec2Config *awstypes.Ec2Config) []any {
	if ec2Config == nil {
		return []any{}
	}

	m := map[string]any{
		"security_group_arns": flex.FlattenStringValueSet(ec2Config.SecurityGroupArns),
		"subnet_arn":          aws.ToString(ec2Config.SubnetArn),
	}

	return []any{m}
}
