func flattenSubDomains(apiObjects []types.SubDomain) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenSubDomain(apiObject))
	}

	return tfList
}
