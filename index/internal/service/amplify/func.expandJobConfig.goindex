func expandJobConfig(tfMap map[string]any) *types.JobConfig {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.JobConfig{}

	if v, ok := tfMap["build_compute_type"].(string); ok && v != "" {
		apiObject.BuildComputeType = types.BuildComputeType(v)
	}

	return apiObject
}
