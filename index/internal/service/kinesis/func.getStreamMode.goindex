func getStreamMode(d sdkv2.ResourceDiffer) types.StreamMode {
	streamMode, ok := d.GetOk("stream_mode_details.0.stream_mode")
	if !ok {
		return types.StreamModeProvisioned
	}

	return types.StreamMode(streamMode.(string))
}
