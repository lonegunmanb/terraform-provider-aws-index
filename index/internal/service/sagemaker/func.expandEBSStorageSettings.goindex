func expandEBSStorageSettings(l []any) *awstypes.EbsStorageSettings {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	config := &awstypes.EbsStorageSettings{}

	if v, ok := m["ebs_volume_size_in_gb"].(int); ok {
		config.EbsVolumeSizeInGb = aws.Int32(int32(v))
	}

	return config
}
