func expandImageVersionResourceID(id string) (string, int, error) {
	parts, err := flex.ExpandResourceId(id, imageVersionResourcePartCount, false)
	if err != nil {
		return "", 0, err
	}

	name := parts[0]
	version, err := strconv.Atoi(parts[1])
	if err != nil {
		return name, 0, err
	}

	return name, version, nil
}
