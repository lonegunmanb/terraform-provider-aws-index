func expandWorkforceCognitoConfig(l []any) *awstypes.CognitoConfig {
	if len(l) == 0 || l[0] == nil {
		return nil
	}

	m := l[0].(map[string]any)

	config := &awstypes.CognitoConfig{
		ClientId: aws.String(m[names.AttrClientID].(string)),
		UserPool: aws.String(m["user_pool"].(string)),
	}

	return config
}
