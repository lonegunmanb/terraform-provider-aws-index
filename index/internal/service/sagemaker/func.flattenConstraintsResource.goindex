func flattenConstraintsResource(config *awstypes.MonitoringConstraintsResource) []map[string]any {
	if config == nil {
		return []map[string]any{}
	}

	m := map[string]any{}

	if config.S3Uri != nil {
		m["s3_uri"] = aws.ToString(config.S3Uri)
	}

	return []map[string]any{m}
}
