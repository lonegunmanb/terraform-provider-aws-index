func flattenModelDataSource(apiObject *awstypes.ModelDataSource) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := make(map[string]any)
	if apiObject.S3DataSource != nil {
		tfMap["s3_data_source"] = flattenS3ModelDataSource(apiObject.S3DataSource)
	}

	return []any{tfMap}
}
