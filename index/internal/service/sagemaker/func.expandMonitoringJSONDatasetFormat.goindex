func expandMonitoringJSONDatasetFormat(configured []any) *awstypes.MonitoringJsonDatasetFormat {
	if len(configured) == 0 {
		return nil
	}

	c := &awstypes.MonitoringJsonDatasetFormat{}

	if configured[0] == nil {
		return c
	}

	m := configured[0].(map[string]any)
	if v, ok := m["line"]; ok {
		c.Line = aws.Bool(v.(bool))
	}

	return c
}
