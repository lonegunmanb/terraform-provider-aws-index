func decodeDeviceId(id string) (string, string, error) {
	iDParts := strings.Split(id, "/")
	if len(iDParts) != 2 {
		return "", "", fmt.Errorf("unexpected format of ID (%q), expected DEVICE-FLEET-NAME:DEVICE-NAME", id)
	}
	return iDParts[0], iDParts[1], nil
}
