func flattenCapacitySize(apiObject *awstypes.CapacitySize) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		names.AttrType:  apiObject.Type,
		names.AttrValue: aws.ToInt32(apiObject.Value),
	}

	return []any{tfMap}
}
