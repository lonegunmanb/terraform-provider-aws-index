func flattenProvisionedCapacityDescription(apiObject *awstypes.ProvisionedCapacityDescription) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"mcu_count":    apiObject.McuCount,
		"worker_count": apiObject.WorkerCount,
	}

	return tfMap
}
