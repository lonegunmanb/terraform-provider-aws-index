func flattenOrders(os []awstypes.Order) []map[string]any {
	orders := make([]map[string]any, len(os))
	for i, v := range os {
		order := make(map[string]any)
		order["column"] = aws.ToString(v.Column)
		order["sort_order"] = int(v.SortOrder)
		orders[i] = order
	}

	return orders
}
