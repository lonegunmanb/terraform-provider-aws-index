func flattenMLTransformSchemaColumns(schemaCols []awstypes.SchemaColumn) []any {
	l := []any{}

	for _, schemaCol := range schemaCols {
		m := map[string]any{
			names.AttrName: aws.ToString(schemaCol.Name),
			"data_type":    aws.ToString(schemaCol.DataType),
		}

		l = append(l, m)
	}

	return l
}
