func flattenMultiRegionKeys(apiObjects []awstypes.MultiRegionKey) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenMultiRegionKey(&apiObject))
	}

	return tfList
}
