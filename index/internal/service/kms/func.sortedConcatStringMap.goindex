func sortedConcatStringMap(m map[string]string) string {
	keys := tfmaps.Keys(m)
	slices.Sort(keys)

	var elems []string
	for _, key := range keys {
		elems = append(elems, key, m[key])
	}

	return strings.Join(elems, "-")
}
