func newNotificationHubResource(context.Context) (resource.ResourceWithConfigure, error) {
	r := &notificationHubResource{}

	r.SetDefaultCreateTimeout(20 * time.Minute)
	r.SetDefaultDeleteTimeout(20 * time.Minute)

	return r, nil
}
