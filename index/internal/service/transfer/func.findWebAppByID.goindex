func findWebAppByID(ctx context.Context, conn *transfer.Client, id string) (*awstypes.DescribedWebApp, error) {
	input := transfer.DescribeWebAppInput{
		WebAppId: aws.String(id),
	}

	return findWebApp(ctx, conn, &input)
}
