func flattenS3Tags(apiObjects []awstypes.S3Tag) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		flattenedObject := map[string]any{}

		if v := apiObject.Key; v != nil {
			flattenedObject[names.AttrKey] = aws.ToString(v)
		}

		if v := apiObject.Value; v != nil {
			flattenedObject[names.AttrValue] = aws.ToString(v)
		}

		tfList = append(tfList, flattenedObject)
	}

	return tfList
}
