func flattenPOSIXProfile(apiObject *awstypes.PosixProfile) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"gid":            aws.ToInt64(apiObject.Gid),
		"secondary_gids": apiObject.SecondaryGids,
		"uid":            aws.ToInt64(apiObject.Uid),
	}

	return []any{tfMap}
}
