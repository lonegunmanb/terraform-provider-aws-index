func flattenEnabledMetrics(apiObjects []awstypes.EnabledMetric) []string {
	var tfList []string

	for _, apiObject := range apiObjects {
		if v := apiObject.Metric; v != nil {
			tfList = append(tfList, aws.ToString(v))
		}
	}

	return tfList
}
