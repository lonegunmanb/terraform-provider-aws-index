func setTagsOut(ctx context.Context, tags any, identifier, resourceType string) {
	if inContext, ok := tftags.FromContext(ctx); ok {
		inContext.TagsOut = option.Some(KeyValueTags(ctx, tags, identifier, resourceType))
	}
}
