package github.com/hashicorp/terraform-provider-aws/internal/service/storagegateway
import (
	"fmt"

	"github.com/YakDriver/regexache"
)
func validLinuxFileMode(v any, k string) (ws []string, errors []error) {
	value := v.(string)
	if !regexache.MustCompile(`^[0-7]{4}$`).MatchString(value) {
		errors = append(errors, fmt.Errorf(
			"only valid linux mode is allowed in %q", k))
	}
	return
}
