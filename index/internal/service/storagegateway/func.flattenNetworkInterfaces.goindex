func flattenNetworkInterfaces(apiObjects []awstypes.NetworkInterface) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfMap := map[string]any{
			"ipv4_address": aws.ToString(apiObject.Ipv4Address),
		}

		tfList = append(tfList, tfMap)
	}

	return tfList
}
