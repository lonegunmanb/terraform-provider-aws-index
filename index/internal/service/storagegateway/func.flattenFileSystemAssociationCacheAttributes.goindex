func flattenFileSystemAssociationCacheAttributes(apiObject *awstypes.CacheAttributes) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"cache_stale_timeout_in_seconds": aws.ToInt32(apiObject.CacheStaleTimeoutInSeconds),
	}

	return []any{tfMap}
}
