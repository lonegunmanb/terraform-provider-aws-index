func flattenServerlessSasl(apiObject *types.ServerlessSasl) map[string]any { // nosemgrep:ci.caps2-in-func-name
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.Iam; v != nil {
		tfMap["iam"] = []any{flattenIam(v)}
	}

	return tfMap
}
