func expandIam(tfMap map[string]any) *types.Iam { // nosemgrep:ci.caps4-in-func-name
	if tfMap == nil {
		return nil
	}

	apiObject := &types.Iam{}

	if v, ok := tfMap[names.AttrEnabled].(bool); ok {
		apiObject.Enabled = aws.Bool(v)
	}

	return apiObject
}
