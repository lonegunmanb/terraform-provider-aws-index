func flattenStorageInfo(apiObject *types.StorageInfo) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.EbsStorageInfo; v != nil {
		tfMap["ebs_storage_info"] = flattenEBSStorageInfo(v)
	}

	return []any{tfMap}
}
