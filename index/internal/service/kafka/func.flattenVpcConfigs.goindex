func flattenVpcConfigs(apiObjects []types.VpcConfig) []any { // nosemgrep:ci.caps5-in-func-name
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenVpcConfig(apiObject))
	}

	return tfList
}
