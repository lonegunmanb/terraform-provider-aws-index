func flattenScanningConfigurationFilters(l []types.ScanningRepositoryFilter) []any {
	if len(l) == 0 {
		return nil
	}

	out := make([]any, len(l))
	for i, filter := range l {
		out[i] = map[string]any{
			names.AttrFilter: aws.ToString(filter.Filter),
			"filter_type":    filter.FilterType,
		}
	}

	return out
}
