func flattenImageScanningConfiguration(isc *types.ImageScanningConfiguration) []map[string]any {
	if isc == nil {
		return nil
	}

	config := make(map[string]any)
	config["scan_on_push"] = isc.ScanOnPush

	return []map[string]any{
		config,
	}
}
