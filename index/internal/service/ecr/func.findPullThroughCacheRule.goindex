func findPullThroughCacheRule(ctx context.Context, conn *ecr.Client, input *ecr.DescribePullThroughCacheRulesInput) (*types.PullThroughCacheRule, error) {
	output, err := findPullThroughCacheRules(ctx, conn, input)

	if err != nil {
