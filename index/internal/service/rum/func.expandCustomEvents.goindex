func expandCustomEvents(tfMap map[string]any) *awstypes.CustomEvents {
	if tfMap == nil {
		return nil
	}

	config := &awstypes.CustomEvents{}

	if v, ok := tfMap[names.AttrStatus].(string); ok && v != "" {
		config.Status = awstypes.CustomEventsStatus(v)
	}

	return config
}
