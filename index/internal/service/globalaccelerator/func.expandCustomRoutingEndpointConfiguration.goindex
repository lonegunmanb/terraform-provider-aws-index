func expandCustomRoutingEndpointConfiguration(tfMap map[string]any) *awstypes.CustomRoutingEndpointConfiguration {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.CustomRoutingEndpointConfiguration{}

	if v, ok := tfMap["endpoint_id"].(string); ok && v != "" {
		apiObject.EndpointId = aws.String(v)
	}

	return apiObject
}
