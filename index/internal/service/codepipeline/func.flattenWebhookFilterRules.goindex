func flattenWebhookFilterRules(filters []types.WebhookFilterRule) []any {
	results := []any{}
	for _, filter := range filters {
		f := map[string]any{
			"json_path":    aws.ToString(filter.JsonPath),
			"match_equals": aws.ToString(filter.MatchEquals),
		}
		results = append(results, f)
	}

	return results
}
