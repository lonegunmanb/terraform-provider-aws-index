func expandConfiguration(tfMap map[string]any) *awstypes.MapConfiguration {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.MapConfiguration{}

	if v, ok := tfMap["style"].(string); ok && v != "" {
		apiObject.Style = aws.String(v)
	}

	return apiObject
}
