func flattenDataSourceConfiguration(apiObject *awstypes.DataSourceConfiguration) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"intended_use": string(apiObject.IntendedUse),
	}

	return tfMap
}
