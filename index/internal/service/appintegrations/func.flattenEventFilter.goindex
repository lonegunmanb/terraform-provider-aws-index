func flattenEventFilter(eventFilter *awstypes.EventFilter) []any {
	if eventFilter == nil {
		return []any{}
	}

	values := map[string]any{
		names.AttrSource: aws.ToString(eventFilter.Source),
	}

	return []any{values}
}
