func subnetIdsToSlice(subnetIds []awstypes.Subnet) []string {
	subnetsSlice := make([]string, 0, len(subnetIds))
	for _, s := range subnetIds {
		subnetsSlice = append(subnetsSlice, aws.ToString(s.SubnetIdentifier))
	}
	return subnetsSlice
}
