func flattenClusterNodes(apiObjects []awstypes.ClusterNode) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenClusterNode(apiObject))
	}

	return tfList
}
