func flattenAliasRoutingConfiguration(apiObject *awstypes.AliasRoutingConfiguration) []any {
	if apiObject == nil {
		return []any{}
	}

	tfMap := map[string]any{
		"additional_version_weights": apiObject.AdditionalVersionWeights,
	}

	return []any{tfMap}
}
