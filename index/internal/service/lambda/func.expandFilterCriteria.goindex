func expandFilterCriteria(tfMap map[string]any) *awstypes.FilterCriteria {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.FilterCriteria{}

	if v, ok := tfMap[names.AttrFilter].(*schema.Set); ok && v.Len() > 0 {
		apiObject.Filters = expandFilters(v.List())
	}

	return apiObject
}
