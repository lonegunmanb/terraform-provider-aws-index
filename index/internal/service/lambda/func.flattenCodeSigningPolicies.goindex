func flattenCodeSigningPolicies(apiObject *awstypes.CodeSigningPolicies) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		"untrusted_artifact_on_deployment": apiObject.UntrustedArtifactOnDeployment,
	}

	return []any{tfMap}
}
