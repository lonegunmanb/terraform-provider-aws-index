func flattenUserIdentityConfiguration(apiObject *types.UserIdentityConfiguration) []any {
	if apiObject == nil {
		return nil
	}

	m := make(map[string]any)

	if v := apiObject.IdentityAttributeName; v != nil {
		m["identity_attribute_name"] = aws.ToString(v)
	}

	return []any{m}
}
