func flattenS3Path(apiObject *types.S3Path) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.Bucket; v != nil {
		m[names.AttrBucket] = aws.ToString(v)
	}

	if v := apiObject.Key; v != nil {
		m[names.AttrKey] = aws.ToString(v)
	}

	return []any{m}
}
