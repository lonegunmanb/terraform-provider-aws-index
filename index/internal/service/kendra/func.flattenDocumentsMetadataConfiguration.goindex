func flattenDocumentsMetadataConfiguration(apiObject *types.DocumentsMetadataConfiguration) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.S3Prefix; v != nil {
		tfMap["s3_prefix"] = aws.ToString(v)
	}

	return []any{tfMap}
}
