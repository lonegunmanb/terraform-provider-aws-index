func expandSAPODataPaginationConfigProperties(tfMap map[string]any) *types.SAPODataPaginationConfig {
	if tfMap == nil {
		return nil
	}

	a := &types.SAPODataPaginationConfig{}

	if v, ok := tfMap["max_page_size"].(int); ok && v != 0 {
		a.MaxPageSize = aws.Int32(int32(v))
	}

	return a
}
