func expandS3InputFormatConfig(tfMap map[string]any) *types.S3InputFormatConfig {
	if tfMap == nil {
		return nil
	}

	a := &types.S3InputFormatConfig{}

	if v, ok := tfMap["s3_input_file_type"].(string); ok && v != "" {
		a.S3InputFileType = types.S3InputFileType(v)
	}

	return a
}
