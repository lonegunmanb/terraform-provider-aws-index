func flattenVPCConfig(apiObject *types.VpcConfig) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{
		names.AttrSecurityGroupIDs: flex.FlattenStringValueSet(apiObject.SecurityGroupIds),
		names.AttrSubnets:          flex.FlattenStringValueSet(apiObject.Subnets),
	}

	return []any{m}
}
