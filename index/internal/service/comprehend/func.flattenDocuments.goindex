func flattenDocuments(apiObject *types.EntityRecognizerDocuments) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{
		"s3_uri":       aws.ToString(apiObject.S3Uri),
		"input_format": apiObject.InputFormat,
	}

	if v := apiObject.TestS3Uri; v != nil {
		m["test_s3_uri"] = aws.ToString(v)
	}

	return []any{m}
}
