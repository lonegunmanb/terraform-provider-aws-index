func flattenOcspConfiguration(config *types.OcspConfiguration) []any {
	if config == nil {
		return []any{}
	}

	m := map[string]any{
		names.AttrEnabled:   aws.ToBool(config.Enabled),
		"ocsp_custom_cname": aws.ToString(config.OcspCustomCname),
	}

	return []any{m}
}
