func flattenAccessScope(apiObject *types.AccessScope) []any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{
		names.AttrType: (*string)(&apiObject.Type),
		"namespaces":   apiObject.Namespaces,
	}

	return []any{tfMap}
}
