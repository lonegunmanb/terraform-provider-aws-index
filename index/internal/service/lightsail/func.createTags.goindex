func createTags(ctx context.Context, conn *lightsail.Client, identifier string, tags []awstypes.Tag, optFns ...func(*lightsail.Options)) error {
	if len(tags) == 0 {
		return nil
	}

	return updateTags(ctx, conn, identifier, nil, keyValueTags(ctx, tags), optFns...)
}
