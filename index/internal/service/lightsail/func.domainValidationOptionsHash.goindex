func domainValidationOptionsHash(v any) int {
	m, ok := v.(map[string]any)

	if !ok {
		return 0
	}

	if v, ok := m[names.AttrDomainName].(string); ok {
		return create.StringHashcode(v)
	}

	return 0
}
