func expandECRImagePullerRole(tfMap map[string]any) *types.ContainerServiceECRImagePullerRoleRequest {
	if tfMap == nil {
		return nil
	}

	apiObject := &types.ContainerServiceECRImagePullerRoleRequest{}

	if v, ok := tfMap["is_active"].(bool); ok {
		apiObject.IsActive = aws.Bool(v)
	}

	return apiObject
}
