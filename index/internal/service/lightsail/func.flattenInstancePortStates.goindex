func flattenInstancePortStates(apiObjects []types.InstancePortState) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		// if apiObject == nil {
		// 	continue
		// }

		tfList = append(tfList, flattenInstancePortState(apiObject))
	}

	return tfList
}
