func validateMonthlySpend(v any, k string) (ws []string, errors []error) {
	vInt := v.(int)
	if vInt < 0 {
		errors = append(errors, fmt.Errorf("setting SMS preferences: monthly spend limit value [%d] must be >= 0", vInt))
	}
	return
}
