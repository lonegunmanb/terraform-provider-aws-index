func isChangeSha256Removal(oldRaw, newRaw any) bool {
	old, ok := oldRaw.(string)
	if !ok {
		return false
	}

	new, ok := newRaw.(string)
	if !ok {
		return false
	}

	hash := sha256.Sum256([]byte(new))
	return hex.EncodeToString(hash[:]) == old
}
