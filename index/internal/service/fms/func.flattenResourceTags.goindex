func flattenResourceTags(apiObjects []awstypes.ResourceTag) map[string]any {
	tfMap := map[string]any{}

	for _, v := range apiObjects {
		tfMap[aws.ToString(v.Key)] = aws.ToString(v.Value)
	}

	return tfMap
}
