func findAnycastIPListByID(ctx context.Context, conn *cloudfront.Client, id string) (*cloudfront.GetAnycastIpListOutput, error) {
	input := cloudfront.GetAnycastIpListInput{
		Id: aws.String(id),
	}

	return findAnycastIPList(ctx, conn, &input)
}
