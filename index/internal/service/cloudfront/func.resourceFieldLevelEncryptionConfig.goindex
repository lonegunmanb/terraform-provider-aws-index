							MaxItems: 1,
							Elem: &schema.Resource{
								Schema: map[string]*schema.Schema{
									"items": {
										Type:     schema.TypeSet,
										Required: true,
										Elem: &schema.Resource{
											Schema: map[string]*schema.Schema{
												names.AttrContentType: {
													Type:     schema.TypeString,
													Required: true,
												},
												names.AttrFormat: {
													Type:             schema.TypeString,
													Required:         true,
