func flattenLambdaFunctionAssociations(apiObject *awstypes.LambdaFunctionAssociations) []any {
	if apiObject == nil {
		return nil
	}

	var tfList []any

	for _, v := range apiObject.Items {
		tfList = append(tfList, flattenLambdaFunctionAssociation(&v))
	}

	return tfList
}
