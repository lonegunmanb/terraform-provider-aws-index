func flattenEndpointEventBuses(apiObjects []types.EndpointEventBus) []any {
	if len(apiObjects) == 0 {
		return nil
	}

	var tfList []any

	for _, apiObject := range apiObjects {
		tfList = append(tfList, flattenEndpointEventBus(&apiObject))
	}

	return tfList
}
