package github.com/hashicorp/terraform-provider-aws/internal/service/events
import (
	"context"

	"github.com/hashicorp/terraform-plugin-sdk/v2/helper/schema"
	"github.com/hashicorp/terraform-provider-aws/names"
)
func targetStateUpgradeV0(_ context.Context, rawState map[string]any, meta any) (map[string]any, error) {
	if rawState == nil {
		rawState = map[string]any{}
	}

	if _, ok := rawState["event_bus_name"]; !ok {
		rawState["event_bus_name"] = DefaultEventBusName
	}

	return rawState, nil
}
