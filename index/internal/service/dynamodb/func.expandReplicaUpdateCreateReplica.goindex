func expandReplicaUpdateCreateReplica(tfMap map[string]any) *awstypes.ReplicaUpdate {
	apiObject := &awstypes.ReplicaUpdate{
		Create: &awstypes.CreateReplicaAction{
			RegionName: aws.String(tfMap["region_name"].(string)),
		},
	}

	return apiObject
}
