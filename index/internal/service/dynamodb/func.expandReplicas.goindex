func expandReplicas(tfList []any) []awstypes.Replica {
	apiObjects := make([]awstypes.Replica, 0, len(tfList))

	for _, tfMapRaw := range tfList {
		tfMap := tfMapRaw.(map[string]any)
		apiObjects = append(apiObjects, *expandReplica(tfMap))
	}

	return apiObjects
}
