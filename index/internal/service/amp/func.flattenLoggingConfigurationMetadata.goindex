func flattenLoggingConfigurationMetadata(apiObject *types.LoggingConfigurationMetadata) map[string]any {
	if apiObject == nil {
		return nil
	}

	tfMap := map[string]any{}

	if v := apiObject.LogGroupArn; v != nil {
		tfMap["log_group_arn"] = aws.ToString(v)
	}

	return tfMap
}
