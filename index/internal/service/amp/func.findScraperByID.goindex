func findScraperByID(ctx context.Context, conn *amp.Client, id string) (*awstypes.ScraperDescription, error) {
	input := amp.DescribeScraperInput{
		ScraperId: aws.String(id),
	}

	return findScraper(ctx, conn, &input)
}
