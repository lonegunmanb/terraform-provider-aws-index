func flattenCapacityConfiguration(apiObject *types.CapacityConfiguration) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.NodeType; v != nil {
		m["node_type"] = aws.ToString(v)
	}

	if v := apiObject.NodeCount; v != nil {
		m["node_count"] = aws.ToInt32(v)
	}

	return []any{m}
}
