func flattenIcmpTypeCode(apiObject *types.IcmpTypeCode) []any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{
		names.AttrType: apiObject.Type,
		"code":         apiObject.Code,
	}

	return []any{m}
}
