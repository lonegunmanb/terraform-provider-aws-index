func flattenCacheConfiguration(apiObject *types.KxDatabaseCacheConfiguration) map[string]any {
	if apiObject == nil {
		return nil
	}

	m := map[string]any{}

	if v := apiObject.CacheType; aws.ToString(v) != "" {
		m["cache_type"] = aws.ToString(v)
	}

	if v := apiObject.DbPaths; v != nil {
		m["db_paths"] = v
	}

	return m
}
