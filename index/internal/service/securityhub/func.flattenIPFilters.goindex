func flattenIPFilters(filters []types.IpFilter) []any {
	if len(filters) == 0 {
		return nil
	}

	var ipFilters []any

	for _, filter := range filters {
		m := map[string]any{
			"cidr": aws.ToString(filter.Cidr),
		}

		ipFilters = append(ipFilters, m)
	}

	return ipFilters
}
