func keywordFilterSchema() *schema.Schema {
	return &schema.Schema{
		Type:     schema.TypeSet,
		Optional: true,
		MaxItems: 20,
		Elem: &schema.Resource{
			Schema: map[string]*schema.Schema{
				names.AttrValue: {
					Type:     schema.TypeString,
					Required: true,
				},
			},
		},
	}
}
