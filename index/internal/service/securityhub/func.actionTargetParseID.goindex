func actionTargetParseID(arn string) (string, error) {
	parts := strings.Split(arn, "/")

	if len(parts) != 3 {
		return "", fmt.Errorf("expected Security Hub Custom action ARN, received: %s", arn)
	}

	return parts[2], nil
}
