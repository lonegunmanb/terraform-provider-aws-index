func translatePath(in cty.Path) path.Path {
	var out path.Path

	if len(in) == 0 {
		return out
	}

	step := in[0]
	switch v := step.(type) {
	case cty.GetAttrStep:
		out = path.Root(v.Name)
	}

	for i := 1; i < len(in); i++ {
		step := in[i]
		switch v := step.(type) {
		case cty.GetAttrStep:
			out = out.AtName(v.Name)

		case cty.IndexStep:
			switch v.Key.Type() {
			case cty.Number:
				v, _ := v.Key.AsBigFloat().Int64()
				out = out.AtListIndex(int(v))
			case cty.String:
				out = out.AtMapKey(v.Key.AsString())
			}
		}
	}

	return out
}
