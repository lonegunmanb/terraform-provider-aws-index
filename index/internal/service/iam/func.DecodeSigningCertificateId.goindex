func DecodeSigningCertificateId(id string) (string, string, error) {
	creds := strings.Split(id, ":")
	if len(creds) != 2 {
		return "", "", fmt.Errorf("unknown IAM Signing Certificate ID format")
	}

	certId := creds[0]
	userName := creds[1]

	return certId, userName, nil
}
