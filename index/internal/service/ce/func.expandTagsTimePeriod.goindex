func expandTagsTimePeriod(tfMap map[string]any) *awstypes.DateInterval {
	if tfMap == nil {
		return nil
	}

	apiObject := &awstypes.DateInterval{}
	apiObject.Start = aws.String(tfMap["start"].(string))
	apiObject.End = aws.String(tfMap["end"].(string))

	return apiObject
}
