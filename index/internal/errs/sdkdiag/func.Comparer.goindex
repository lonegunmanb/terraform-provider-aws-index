package github.com/hashicorp/terraform-provider-aws/internal/errs/sdkdiag
import (
	"github.com/hashicorp/terraform-plugin-sdk/v2/diag"
)
func Comparer(l, r diag.Diagnostic) bool {
	if l.Severity != r.Severity {
		return false
	}
	if l.Summary != r.Summary {
		return false
	}
	if l.Detail != r.Detail {
		return false
	}

	lp := l.AttributePath
	rp := r.AttributePath
	if len(lp) != len(rp) {
		return false
	}
	return lp.Equals(rp)
}
