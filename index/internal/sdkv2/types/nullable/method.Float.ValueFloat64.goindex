package github.com/hashicorp/terraform-provider-aws/internal/sdkv2/types/nullable
import (
	"fmt"
	"strconv"

	"github.com/hashicorp/terraform-plugin-sdk/v2/helper/schema"
)
func (i Float) ValueFloat64() (float64, bool, error) {
	if i.IsNull() {
		return 0, true, nil
	}

	value, err := strconv.ParseFloat(string(i), 64)
	if err != nil {
		return 0, false, err
	}
	return value, false, nil
}
