package github.com/hashicorp/terraform-provider-aws/internal/sdkv2/types/nullable
import (
	"fmt"
	"strconv"

	"github.com/hashicorp/terraform-plugin-sdk/v2/helper/schema"
)
func DiffSuppressNullableBool(k, o, n string, d *schema.ResourceData) bool {
	ov, onull, _ := Bool(o).ValueBool()
	nv, nnull, _ := Bool(n).ValueBool()
	if onull && nnull {
		return true
	}
	if !onull && !nnull {
		return ov == nv
	}
	return false
}
