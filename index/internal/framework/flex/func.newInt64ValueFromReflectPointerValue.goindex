func newInt64ValueFromReflectPointerValue(v reflect.Value) (attr.Value, diag.Diagnostics) {
	if v.IsNil() {
		var diags diag.Diagnostics
		return types.Int64Null(), diags
	}

	return newInt64ValueFromReflectValue(v.Elem())
}
