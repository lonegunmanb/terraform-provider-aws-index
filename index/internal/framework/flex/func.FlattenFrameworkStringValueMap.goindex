package github.com/hashicorp/terraform-provider-aws/internal/framework/flex
import (
	"context"

	"github.com/hashicorp/terraform-plugin-framework/attr"
	"github.com/hashicorp/terraform-plugin-framework/types"
	"github.com/hashicorp/terraform-plugin-framework/types/basetypes"
)
func FlattenFrameworkStringValueMap(ctx context.Context, v map[string]string) types.Map {
	if len(v) == 0 {
		return types.MapNull(types.StringType)
	}

	var output types.Map

	must(Flatten(ctx, v, &output))

	return output
}
