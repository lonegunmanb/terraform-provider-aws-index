package github.com/hashicorp/terraform-provider-aws/internal/framework/flex
import (
	"strings"
)
func (o tagOptions) WrapperField() string {
	if len(o) == 0 {
		return ""
	}
	s := string(o)
	for s != "" {
		var option string
		option, s, _ = strings.Cut(s, ",")
		if name, value, found := strings.Cut(option, "="); found && name == "wrapper" {
			return value
		}
	}
	return ""
}
