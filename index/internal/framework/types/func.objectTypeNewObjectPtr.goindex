)
func objectTypeNewObjectPtr[T any](ctx context.Context) (*T, diag.Diagnostics) {
	var diags diag.Diagnostics

	t := new(T)
	diags.Append(NullOutObjectPtrFields(ctx, t)...)
	if diags.HasError() {
		return nil, diags
	}

	return t, diags
}
