package github.com/hashicorp/terraform-provider-aws/internal/framework/types
import (
	"context"
	"encoding/json"
	"fmt"

	"github.com/hashicorp/terraform-plugin-framework-jsontypes/jsontypes"
	"github.com/hashicorp/terraform-plugin-framework/attr"
	"github.com/hashicorp/terraform-plugin-framework/attr/xattr"
	"github.com/hashicorp/terraform-plugin-framework/diag"
	"github.com/hashicorp/terraform-plugin-framework/types/basetypes"
	"github.com/hashicorp/terraform-plugin-go/tftypes"
	smithyjson "github.com/hashicorp/terraform-provider-aws/internal/json"
)
func (v SmithyJSON[T]) ValueInterface() (T, diag.Diagnostics) {
	var diags diag.Diagnostics

	var zero T
	if v.IsNull() || v.IsUnknown() {
		return zero, diags
	}

	var data any
	err := json.Unmarshal([]byte(v.ValueString()), &data)

	if err != nil {
		diags.AddError(
			"JSON Unmarshal Error",
			"An unexpected error occurred while unmarshalling a JSON string. "+
				"Please report this to the provider developers.\n\n"+
				"Error: "+err.Error(),
		)
		return zero, diags
	}

	return v.f(data), diags
}
