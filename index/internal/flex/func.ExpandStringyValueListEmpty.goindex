func ExpandStringyValueListEmpty[E ~string](configured []any) []E {
	vs := make([]E, 0, len(configured))
	for _, v := range configured {
		if val, ok := v.(string); ok { // empty string in config turns into nil in []interface{} so !ok
			vs = append(vs, E(val))
		} else {
			vs = append(vs, E(""))
		}
	}
	return vs
}
