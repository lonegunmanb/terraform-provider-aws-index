package github.com/hashicorp/terraform-provider-aws/internal/sync
import (
	"context"
	"errors"
	"sync"
)
func (g *Group) Go(ctx context.Context, f func(context.Context) error) {
	g.wg.Add(1)
	go func(ctx context.Context) {
		defer g.wg.Done()
		if err := f(ctx); err != nil {
			g.mutex.Lock()
			g.errs = append(g.errs, err)
			g.mutex.Unlock()
		}
	}(ctx)
}
