package github.com/hashicorp/terraform-provider-aws/internal/json
import (
	"reflect"
)
func EqualBytes(b1, b2 []byte) bool {
	var v1 any
	if err := DecodeFromBytes(b1, &v1); err != nil {
		return false
	}

	var v2 any
	if err := DecodeFromBytes(b2, &v2); err != nil {
		return false
	}

	return reflect.DeepEqual(v1, v2)
}
