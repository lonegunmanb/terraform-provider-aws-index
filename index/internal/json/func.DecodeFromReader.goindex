package github.com/hashicorp/terraform-provider-aws/internal/json
import (
	"bytes"
	"encoding/json"
	"io"
	"strings"
)
func DecodeFromReader(r io.Reader, to any) error {
	dec := json.NewDecoder(r)

	for {
		if err := dec.Decode(to); err == io.EOF {
			break
		} else if err != nil {
			return err
		}
	}

	return nil
}
