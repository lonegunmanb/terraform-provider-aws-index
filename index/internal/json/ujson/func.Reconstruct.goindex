package github.com/hashicorp/terraform-provider-aws/internal/json/ujson
import "fmt"
func Reconstruct(input []byte) ([]byte, error) {
	b := make([]byte, 0, len(input))
	err := Walk(input, func(st int, key, value []byte) bool {
		if len(b) != 0 && ShouldAddComma(value, b[len(b)-1]) {
			b = append(b, ',')
		}
		if len(key) > 0 {
			b = append(b, key...)
			b = append(b, ':')
		}
		b = append(b, value...)
		return true
	})
	return b, err
}
