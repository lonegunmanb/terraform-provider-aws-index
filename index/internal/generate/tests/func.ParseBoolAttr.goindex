func ParseBoolAttr(name, value string) (bool, error) {
	if b, err := strconv.ParseBool(value); err != nil {
		return b, fmt.Errorf("invalid %s value %q: Should be boolean value.", name, value)
	} else {
		return b, nil
	}
}
